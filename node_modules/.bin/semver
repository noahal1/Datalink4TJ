#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/noaha/OneDrive/Desktop/work_prj/datalink/Datalink4TJ/node_modules/semver/bin/node_modules:/mnt/c/Users/noaha/OneDrive/Desktop/work_prj/datalink/Datalink4TJ/node_modules/semver/node_modules:/mnt/c/Users/noaha/OneDrive/Desktop/work_prj/datalink/Datalink4TJ/node_modules:/mnt/c/Users/noaha/OneDrive/Desktop/work_prj/datalink/node_modules:/mnt/c/Users/noaha/OneDrive/Desktop/work_prj/node_modules:/mnt/c/Users/noaha/OneDrive/Desktop/node_modules:/mnt/c/Users/noaha/OneDrive/node_modules:/mnt/c/Users/noaha/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/noaha/OneDrive/Desktop/work_prj/datalink/Datalink4TJ/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/noaha/OneDrive/Desktop/work_prj/datalink/Datalink4TJ/node_modules/semver/bin/node_modules:/mnt/c/Users/noaha/OneDrive/Desktop/work_prj/datalink/Datalink4TJ/node_modules/semver/node_modules:/mnt/c/Users/noaha/OneDrive/Desktop/work_prj/datalink/Datalink4TJ/node_modules:/mnt/c/Users/noaha/OneDrive/Desktop/work_prj/datalink/node_modules:/mnt/c/Users/noaha/OneDrive/Desktop/work_prj/node_modules:/mnt/c/Users/noaha/OneDrive/Desktop/node_modules:/mnt/c/Users/noaha/OneDrive/node_modules:/mnt/c/Users/noaha/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/noaha/OneDrive/Desktop/work_prj/datalink/Datalink4TJ/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../semver/bin/semver.js" "$@"
fi
